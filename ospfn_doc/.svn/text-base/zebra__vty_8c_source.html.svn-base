<a name="l00062"></a>00062         }
<a name="l00063"></a>00063       p.<a class="code" href="structprefix.html#aae8dafba4797d25cf1d141780d87ff02">prefixlen</a> = <a class="code" href="prefix_8c.html#ab83ffad6b766ddd0ef87e4da228c322d">ip_masklen</a> (mask);
<a name="l00064"></a>00064     }
<a name="l00065"></a>00065 
<a name="l00066"></a>00066   <span class="comment">/* Apply mask for given prefix. */</span>
<a name="l00067"></a>00067   <a class="code" href="prefix_8c.html#a99265c5e11b1867552cf784a4cac8252">apply_mask</a> (&amp;p);
<a name="l00068"></a>00068 
<a name="l00069"></a>00069   <span class="comment">/* Administrative distance. */</span>
<a name="l00070"></a>00070   <span class="keywordflow">if</span> (distance_str)
<a name="l00071"></a>00071     distance = atoi (distance_str);
<a name="l00072"></a>00072   <span class="keywordflow">else</span>
<a name="l00073"></a>00073     distance = <a class="code" href="zebra_8h.html#a45eb34202f0d8926f22d5c792862d0a2">ZEBRA_STATIC_DISTANCE_DEFAULT</a>;
<a name="l00074"></a>00074 
<a name="l00075"></a>00075   <span class="comment">/* Null0 static route.  */</span>
<a name="l00076"></a>00076   <span class="keywordflow">if</span> ((gate_str != <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>) &amp;&amp; (strncasecmp (gate_str, <span class="stringliteral">&quot;Null0&quot;</span>, strlen (gate_str)) == 0))
<a name="l00077"></a>00077     {
<a name="l00078"></a>00078       <span class="keywordflow">if</span> (flag_str)
<a name="l00079"></a>00079         {
