<a name="l00081"></a>00081           <span class="keywordflow">return</span> <a class="code" href="command_8h.html#a92dc6dc30d8ef3651eb9d47a6083b025">CMD_WARNING</a>;
<a name="l00082"></a>00082         }
<a name="l00083"></a>00083       <span class="keywordflow">if</span> (add_cmd)
<a name="l00084"></a>00084         <a class="code" href="rib_8h.html#a44bb955baa5782ba016fbc81c9810927">static_add_ipv4</a> (&amp;p, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="zebra_8h.html#ab3b7a4bd090244ae1b882b9003810713">ZEBRA_FLAG_BLACKHOLE</a>, distance, 0);
<a name="l00085"></a>00085       <span class="keywordflow">else</span>
<a name="l00086"></a>00086         <a class="code" href="rib_8h.html#a680270ecb5a14879953840c281d92fe8">static_delete_ipv4</a> (&amp;p, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, distance, 0);
<a name="l00087"></a>00087       <span class="keywordflow">return</span> <a class="code" href="command_8h.html#aa2008c1bc36d9ef42571a6ec52516d02">CMD_SUCCESS</a>;
<a name="l00088"></a>00088     }
<a name="l00089"></a>00089 
<a name="l00090"></a>00090   <span class="comment">/* Route flags */</span>
<a name="l00091"></a>00091   <span class="keywordflow">if</span> (flag_str) {
<a name="l00092"></a>00092     <span class="keywordflow">switch</span>(flag_str[0]) {
<a name="l00093"></a>00093       <span class="keywordflow">case</span> <span class="charliteral">&#39;r&#39;</span>:
<a name="l00094"></a>00094       <span class="keywordflow">case</span> <span class="charliteral">&#39;R&#39;</span>: <span class="comment">/* XXX */</span>
<a name="l00095"></a>00095         <a class="code" href="zebra_8h.html#a7c593725d5bbf27a37a922ae84b02333">SET_FLAG</a> (flag, <a class="code" href="zebra_8h.html#ade86fda8e343fab895b8996b27358ca0">ZEBRA_FLAG_REJECT</a>);
<a name="l00096"></a>00096         <span class="keywordflow">break</span>;
<a name="l00097"></a>00097       <span class="keywordflow">case</span> <span class="charliteral">&#39;b&#39;</span>:
<a name="l00098"></a>00098       <span class="keywordflow">case</span> <span class="charliteral">&#39;B&#39;</span>: <span class="comment">/* XXX */</span>
<a name="l00099"></a>00099         <a class="code" href="zebra_8h.html#a7c593725d5bbf27a37a922ae84b02333">SET_FLAG</a> (flag, <a class="code" href="zebra_8h.html#ab3b7a4bd090244ae1b882b9003810713">ZEBRA_FLAG_BLACKHOLE</a>);
<a name="l00100"></a>00100         <span class="keywordflow">break</span>;
<a name="l00101"></a>00101       <span class="keywordflow">default</span>:
<a name="l00102"></a>00102         <a class="code" href="vty_8c.html#a3725db0ec6ad3ffdd31f68abc807ee34">vty_out</a> (vty, <span class="stringliteral">&quot;%% Malformed flag %s %s&quot;</span>, flag_str, <a class="code" href="vty_8h.html#a3be8ca867e2953936a95c68dc64824b6">VTY_NEWLINE</a>);
<a name="l00103"></a>00103         <span class="keywordflow">return</span> <a class="code" href="command_8h.html#a92dc6dc30d8ef3651eb9d47a6083b025">CMD_WARNING</a>;
<a name="l00104"></a>00104     }
<a name="l00105"></a>00105   }
<a name="l00106"></a>00106 
<a name="l00107"></a>00107   <span class="keywordflow">if</span> (gate_str == <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>)
<a name="l00108"></a>00108   {
<a name="l00109"></a>00109     <span class="keywordflow">if</span> (add_cmd)
<a name="l00110"></a>00110       <a class="code" href="rib_8h.html#a44bb955baa5782ba016fbc81c9810927">static_add_ipv4</a> (&amp;p, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, flag, distance, 0);
<a name="l00111"></a>00111     <span class="keywordflow">else</span>
<a name="l00112"></a>00112       <a class="code" href="rib_8h.html#a680270ecb5a14879953840c281d92fe8">static_delete_ipv4</a> (&amp;p, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, distance, 0);
<a name="l00113"></a>00113 
<a name="l00114"></a>00114     <span class="keywordflow">return</span> <a class="code" href="command_8h.html#aa2008c1bc36d9ef42571a6ec52516d02">CMD_SUCCESS</a>;
<a name="l00115"></a>00115   }
<a name="l00116"></a>00116   
<a name="l00117"></a>00117   <span class="comment">/* When gateway is A.B.C.D format, gate is treated as nexthop</span>
<a name="l00118"></a>00118 <span class="comment">     address other case gate is treated as interface name. */</span>
<a name="l00119"></a>00119   ret = <a class="code" href="sockunion_8c.html#a4203367afaacee3fa5cc1626b2ef63c0">inet_aton</a> (gate_str, &amp;gate);
<a name="l00120"></a>00120   <span class="keywordflow">if</span> (ret)
<a name="l00121"></a>00121     ifname = <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>;
<a name="l00122"></a>00122   <span class="keywordflow">else</span>
<a name="l00123"></a>00123     ifname = gate_str;
<a name="l00124"></a>00124 
<a name="l00125"></a>00125   <span class="keywordflow">if</span> (add_cmd)
<a name="l00126"></a>00126     <a class="code" href="rib_8h.html#a44bb955baa5782ba016fbc81c9810927">static_add_ipv4</a> (&amp;p, ifname ? <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a> : &amp;gate, ifname, flag, distance, 0);
<a name="l00127"></a>00127   <span class="keywordflow">else</span>
<a name="l00128"></a>00128     <a class="code" href="rib_8h.html#a680270ecb5a14879953840c281d92fe8">static_delete_ipv4</a> (&amp;p, ifname ? <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a> : &amp;gate, ifname, distance, 0);
<a name="l00129"></a>00129 
<a name="l00130"></a>00130   <span class="keywordflow">return</span> <a class="code" href="command_8h.html#aa2008c1bc36d9ef42571a6ec52516d02">CMD_SUCCESS</a>;
<a name="l00131"></a>00131 }
<a name="l00132"></a>00132 
<a name="l00133"></a>00133 <span class="comment">/* Static route configuration.  */</span>
<a name="l00134"></a><a class="code" href="zebra__vty_8c.html#a879f6d3e87bb075c22668fd3625bf459">00134</a> <a class="code" href="command_8h.html#acc29190d35966eddbcfeceed3aab6d25">DEFUN</a> (ip_route, 
<a name="l00135"></a>00135        ip_route_cmd,
<a name="l00136"></a>00136        <span class="stringliteral">&quot;ip route A.B.C.D/M (A.B.C.D|INTERFACE|null0)&quot;</span>,
<a name="l00137"></a>00137        <a class="code" href="command_8h.html#af45438f811eb6711be47ea2555b12f75">IP_STR</a>
<a name="l00138"></a>00138        <span class="stringliteral">&quot;Establish static routes\n&quot;</span>
<a name="l00139"></a>00139        <span class="stringliteral">&quot;IP destination prefix (e.g. 10.0.0.0/8)\n&quot;</span>
<a name="l00140"></a>00140        <span class="stringliteral">&quot;IP gateway address\n&quot;</span>
<a name="l00141"></a>00141        <span class="stringliteral">&quot;IP gateway interface name\n&quot;</span>
<a name="l00142"></a>00142        <span class="stringliteral">&quot;Null interface\n&quot;</span>)
<a name="l00143"></a>00143 {
<a name="l00144"></a>00144   <span class="keywordflow">return</span> <a class="code" href="zebra__vty_8c.html#a5dd87a2f111cd8b5d8ab264105f0df31">zebra_static_ipv4</a> (<a class="code" href="structvty.html">vty</a>, 1, argv[0], <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, argv[1], <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>, <a class="code" href="getopt1_8c.html#a070d2ce7b6bb7e5c05602aa8c308d0c4">NULL</a>);
<a name="l00145"></a>00145 }
<a name="l00146"></a>00146 
<a name="l00147"></a><a class="code" href="zebra__vty_8c.html#a94a2ebe17cf8674a840437c75390496e">00147</a> <a class="code" href="command_8h.html#acc29190d35966eddbcfeceed3aab6d25">DEFUN</a> (ip_route_flags,
<a name="l00148"></a>00148        ip_route_flags_cmd,
<a name="l00149"></a>00149        <span class="stringliteral">&quot;ip route A.B.C.D/M (A.B.C.D|INTERFACE) (reject|blackhole)&quot;</span>,
<a name="l00150"></a>00150        <a class="code" href="command_8h.html#af45438f811eb6711be47ea2555b12f75">IP_STR</a>
<a name="l00151"></a>00151        <span class="stringliteral">&quot;Establish static routes\n&quot;</span>
<a name="l00152"></a>00152        <span class="stringliteral">&quot;IP destination prefix (e.g. 10.0.0.0/8)\n&quot;</span>
<a name="l00153"></a>00153        <span class="stringliteral">&quot;IP gateway address\n&quot;</span>
<a name="l00154"></a>00154        <span class="stringliteral">&quot;IP gateway interface name\n&quot;</span>
<a name="l00155"></a>00155        <span class="stringliteral">&quot;Emit an ICMP unreachable when matched\n&quot;</span>
