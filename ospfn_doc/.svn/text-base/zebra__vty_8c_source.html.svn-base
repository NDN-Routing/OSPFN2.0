<a name="l00043"></a>00043   <span class="keyword">struct </span>in_addr mask;
<a name="l00044"></a>00044   <span class="keyword">const</span> <span class="keywordtype">char</span> *ifname;
<a name="l00045"></a>00045   u_char flag = 0;
<a name="l00046"></a>00046   
<a name="l00047"></a>00047   ret = <a class="code" href="prefix_8c.html#a35f57d44562d00baaf5d227d03214786">str2prefix</a> (dest_str, &amp;p);
<a name="l00048"></a>00048   <span class="keywordflow">if</span> (ret &lt;= 0)
<a name="l00049"></a>00049     {
<a name="l00050"></a>00050       <a class="code" href="vty_8c.html#a3725db0ec6ad3ffdd31f68abc807ee34">vty_out</a> (vty, <span class="stringliteral">&quot;%% Malformed address%s&quot;</span>, <a class="code" href="vty_8h.html#a3be8ca867e2953936a95c68dc64824b6">VTY_NEWLINE</a>);
<a name="l00051"></a>00051       <span class="keywordflow">return</span> <a class="code" href="command_8h.html#a92dc6dc30d8ef3651eb9d47a6083b025">CMD_WARNING</a>;
<a name="l00052"></a>00052     }
<a name="l00053"></a>00053 
<a name="l00054"></a>00054   <span class="comment">/* Cisco like mask notation. */</span>
<a name="l00055"></a>00055   <span class="keywordflow">if</span> (mask_str)
<a name="l00056"></a>00056     {
<a name="l00057"></a>00057       ret = <a class="code" href="sockunion_8c.html#a4203367afaacee3fa5cc1626b2ef63c0">inet_aton</a> (mask_str, &amp;mask);
<a name="l00058"></a>00058       <span class="keywordflow">if</span> (ret == 0)
<a name="l00059"></a>00059         {
<a name="l00060"></a>00060           <a class="code" href="vty_8c.html#a3725db0ec6ad3ffdd31f68abc807ee34">vty_out</a> (vty, <span class="stringliteral">&quot;%% Malformed address%s&quot;</span>, <a class="code" href="vty_8h.html#a3be8ca867e2953936a95c68dc64824b6">VTY_NEWLINE</a>);
