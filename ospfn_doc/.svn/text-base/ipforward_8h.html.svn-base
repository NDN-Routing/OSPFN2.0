<div class="fragment"><pre class="fragment">{
  <span class="keywordtype">int</span> fd, ret;
  <span class="keywordtype">int</span> af = AF_INET;
  <span class="keywordtype">char</span> netopt[] = <span class="stringliteral">&quot;ipforwarding&quot;</span>;
  <span class="keyword">struct </span>optreq oq;

  fd = socket(af, SOCK_DGRAM, 0);
  <span class="keywordflow">if</span> (fd &lt; 0) {
    <span class="comment">/* need logging here */</span>
    <span class="keywordflow">return</span> -1;
  }

  strcpy (oq.name, netopt);
  oq.getnext = 0;

  ret = ioctl (fd, SIOCGNETOPT, (caddr_t)&amp;oq);
  close(fd);

  <span class="keywordflow">if</span> (ret &lt; 0) {
    <span class="comment">/* need logging here */</span>
    <span class="keywordflow">return</span> -1;
  }

  ret = atoi (oq.data);
  <span class="keywordflow">return</span> ret;
}
